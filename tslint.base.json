{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "curly": [true, "ignore-same-line"],
    "max-classes-per-file": false,
    "no-empty": false,
    "no-shadowed-variable": [
      true,
      {
        "underscore": false
      }
    ],
    "prefer-for-of": false,
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore"
      ]
    }
  },
  "rulesDirectory": []
}
